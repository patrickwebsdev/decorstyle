---
import Layout from '@layouts/Layout.astro';
import '@styles/card-job.scss'
import '@styles/gallery.scss';
import '@styles/utils.scss';
import '@styles/header-service.scss';
import { jobDynamic } from '@config/pisos-flotantes';
import JobCards from '@components/JobCards.astro';
---

<Layout title="DecorStyle | Pisos flotantes">
  <header id="home" class="pisos-flotantes">
    <h1>PISOS FLOTANTES</h1>
  </header>
  <section id="pisos-flotantes" class="mt-5 mb-5">
    <div class="container">
      <JobCards
        jobLimit={jobDynamic.length}
        resource={"pisos-flotantes"}/>
    </div>
  </section>
</Layout>