---
import Layout from '@layouts/Jobs.astro';
import '@styles/card-job.scss'
import '@styles/gallery.scss';
import '@styles/utils.scss';
import '@styles/header-service.scss';
import { jobDynamic } from '@config/construccion-seco';
import JobCards from '@components/JobCards.astro';
---

<Layout title="DecorStyle | Construccion en seco">
  <header id="home" class="construccion-seco">
    <h1>CONSTRUCCION EN SECO</h1>
  </header>
  <section id="construccion-seco" class="mt-5 mb-5">
    <div class="container">
      <JobCards
        jobLimit={jobDynamic.length}
        resource={"construccion-seco"}/>
    </div>
  </section>
</Layout>